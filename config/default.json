{
  "env": "development",
  "server": {
    "host": "http://localhost:3000",
    "port": 3000
  },
  "db": {
    "config": {
      "dialect": "mysql",
      "pool": {
        "max": 10,
        "min": 0,
        "acquire": 30000,
        "idle": 10000
      }
    }
  },
  "cors": {
    "enable": false
  },
  "compression": {
    "enable": true
  },
  "bodyParser": {
    "json": {
      "limit": 50000
    },
    "urlencoded": {
      "limit": 50000,
      "parameterLimit":  50000,
      "extended": false
    }
  },
  "jwtExpireTimeInMin": 60
}